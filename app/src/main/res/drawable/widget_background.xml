<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle" android:innerRadius="@dimen/corner_radius">
            <stroke
                android:width="2dp"
                android:color="@color/brown700"/>
            <corners android:radius="@dimen/corner_radius"/>
            <gradient
                android:angle="90"
                android:endColor="#e6f9d9ad"
                android:startColor="#e6dab05d"/>
        </shape>
    </item>
</layer-list>